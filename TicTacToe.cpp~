#include <iostream>
#include <cstring>

void init(int (&board)[3][3], int& turn);

using namespace std;

void init(int (&board)[3][3], int& turn){
  for(int a = 0; a < 3; a++){
    for(int b = 0; b < 3; b++){
      board[b][a] = 0;
    }
  }
  turn = 0;
}

void printBoard(int (&board)[3][3])

int main(){
  int board[3][3];
  int turn = 0;
  
  int xWins = 0;
  int oWins = 0;
   
  init(board, turn);

  bool running = true;

  cout << board << " " << turn << endl;
}


